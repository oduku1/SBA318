<!DOCTYPE html>
<html>
<head>
  <title>Notes for <%= application.company %> - <%= application.position %></title>
</head>
<body>
  <h1>Notes for <%= application.company %> - <%= application.position %></h1>

  <ul>
    <% applicationNotes.forEach(note => { %>
      <li><%= note.date %>: <%= note.content %></li>
    <% }) %>
  </ul>

  <a href="/applications/<%= application.userName %>/applications">Back to Applications</a>
</body>
</html>